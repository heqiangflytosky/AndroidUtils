<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.circleviewpager.CircleViewPagerActivity">
    <com.android.hq.androidutils.views.circleviewpager.CircleViewPager
        android:id="@+id/view_pager"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@color/black_4"
        app:layout_constraintVertical_chainStyle="packed"
        app:layout_constraintBottom_toTopOf="@id/view_pager1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <com.android.hq.androidutils.views.circleviewpager.CircleViewPager1
        android:id="@+id/view_pager1"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@color/black_4"
        app:layout_constraintTop_toBottomOf="@id/view_pager"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>